  "scripts": {
    "prebuild": "rimraf dist && patch-package",
    "start": "react-scripts start",
    "start:clean": "rm -rf ./node_modules/.cache/babel-loader && react-scripts start",
    "postinstall": "npm run gen && patch-package",
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "npm-run-all build-fragment react-scripts:build",
    "build-fragment": "node src/common/graphql/utils/schemaQuery.js",
    "react-scripts:build": "react-scripts --max-old-space-size=4096 build",
    "test": "react-scripts test --env=jsdom",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "test-all": "npm run eslint && npm run test",
    "eject": "react-scripts eject",
    "gen": "graphql-codegen --config codegen.ts",
    "cypress:login": "npx cypress run --spec cypress/e2e/userLogin.spec.ts",
    "cypress:issue": "npx cypress run --spec cypress/e2e/issueWithAdditionalComment.spec.ts",
    "cypress:space": "npx cypress run --spec cypress/e2e/spaceWithAdditonalCommentTurnedOff.spec.ts",
    "cypress:autoComplete": "npx cypress run --spec cypress/e2e/regressionSuite/autoCompleteAndRadioButton.spec.ts --reporter cypress-mochawesome-reporter",
    "cypress:autoProgress": "npx cypress run --spec cypress/e2e/regressionSuite/autoProgress.spec.ts  --reporter cypress-mochawesome-reporter",
    "cypress:contactInfo": "npx cypress run --spec cypress/e2e/smokeSuite/contactInfo.spec.ts",
    "cypress:surveyForm": "npx cypress run --spec cypress/e2e/surveyForm.spec.ts",
    "cypress:smokeSuite": "npx cypress run --spec cypress/e2e/smokeSuite --record --key `${CYPRESS_RECORD_KEY}` --parallel",
    "cypress:regressionSuite": "npx cypress run --spec cypress/e2e/regressionSuite --record --key `${CYPRESS_RECORD_KEY}` --parallel --reporter cypress-mochawesome-reporter",
    "cypress:e2eSuite": "npx cypress run --spec cypress/e2e/ --record --key `${CYPRESS_RECORD_KEY}` --parallel --reporter cypress-mochawesome-reporter",
    "cypress:parallel": "npm-run-all --parallel cypress:login cypress:issue cypress:space cypress:autoComplete cypress:contactInfo cypress:surveyForm",
    "json:report": "npx mochawesome-merge 'cypress/results/.jsons/*.json' > mochawesome.json",
    "overAllReport": "npx marge ./mochawesome.json --reportDir cypress/results ",
    "cypress:run": "npm run cypress:regressionSuite ",
    "cypress:result": "npm run json:report && npm run overAllReport"
  },
